	author = {Xie, Xuetong and Wen, Ya and Huang, Zhou},
	month = may,
	year = {2017},
	keywords = {distribution histogram of bias, objective function, rotating fan-beam scatterometer, wind direction extension, wind vector retrieval},
	pages = {83--89},
	file = {Full Text PDF:/Users/yuyangzhang/Zotero/storage/HIVYWGHJ/Xie et al. - 2017 - An advanced wind vector retrieval algorithm for th.pdf:application/pdf},
}

