	pages = {5652--5668},
	file = {IEEE Xplore Full Text PDF:/Users/yuyangzhang/Zotero/storage/AMP7TY4C/Xing et al. - 2020 - Mapping Human Activity Volumes Through Remote Sens.pdf:application/pdf},
}

