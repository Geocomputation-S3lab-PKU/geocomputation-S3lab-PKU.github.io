Publisher: Nature Publishing Group},
	keywords = {Statistical physics, Sustainability, thermodynamics and nonlinear dynamics},
	pages = {21201},
	file = {Full Text PDF:/Users/yuyangzhang/Zotero/storage/P9HDSZXT/Dong et al. - 2020 - Understanding the mesoscopic scaling patterns with.pdf:application/pdf},
}

@article{huang_ensemble_2020,
	title = {An {Ensemble} {Learning} {Approach} for {Urban} {Land} {Use} {Mapping} {Based} on {Remote} {Sensing} {Imagery} and {Social} {Sensing} {Data}},
